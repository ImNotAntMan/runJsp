	for(int i =0; i < 5; i++) {
		for(int j = 0; j < 5; j++) {
			out.print("#");		
		}
		out.print("<br>");
	}

	// 구구단 출력
	
	for(int i = 1; i <=9; i++) {
		for(int j = 2; j <= 9; j++) {
			out.print(j + "x" + i + " = " + i*j + "\t");
		}
		out.print("<br>");
	}
%>
